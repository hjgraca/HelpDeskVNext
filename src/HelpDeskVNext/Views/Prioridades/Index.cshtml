@model IEnumerable<HelpDeskVNext.Data.Entitidades.Prioridade>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table table-hover">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Designacao)
        </th>
        <th></th>
    </tr>
    
@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Designacao)
        </td>
        <td>
            <a asp-action="Edit" asp-route-id="@item.PrioridadeId">Edit</a> |
            <a asp-action="Details" asp-route-id="@item.PrioridadeId">Details</a> |
            <a asp-action="Delete" asp-route-id="@item.PrioridadeId">Delete</a>
        </td>
    </tr>
}
</table>
